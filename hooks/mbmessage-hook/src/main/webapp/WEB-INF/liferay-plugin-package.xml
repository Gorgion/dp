<?xml version="1.0"?>
<!DOCTYPE plugin-package PUBLIC "-//Liferay//DTD Plugin Package 6.2.0//EN" "http://www.liferay.com/dtd/liferay-plugin-package_6_2_0.dtd">

<plugin-package>
    <name>${liferay.app.name}</name>
    <module-id>${project.groupId}/${project.artifactId}/${project.version}/war</module-id>
    <types>
        <type>hook</type>
    </types>
    <tags></tags>
    <short-description>${liferay.app.description.short}</short-description>
    <long-description>${liferay.app.description.long}</long-description>
    <change-log></change-log>
    <page-url></page-url>
    <author></author>
    <licenses>
        <license osi-approved="false">Proprietary</license>
    </licenses>
    <liferay-versions>
        <liferay-version>${liferay.version}</liferay-version>
    </liferay-versions>
</plugin-package>